{
  "title": "ROUGEE.PLAY Documentation",
  "description": "Complete documentation for the decentralized music platform",
  "author": "ROUGEE.PLAY Team",
  "language": "en",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search",
    "sharing",
    "fontsettings",
    "highlight",
    "livereload",
    "lunr",
    "search-pro",
    "theme-default"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "styles/website.css"
      }
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "twitter"
      ]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "highlight": {
      "theme": "github"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "chapterMark": "pagebreak",
    "pageBreaksBefore": "/",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}
